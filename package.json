{
  "name": "cotizate-algo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:up": "docker-compose -f docker/docker-compose.yml up -d postgres",
    "db:down": "docker-compose -f docker/docker-compose.yml down",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "test": "jest --runInBand",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration --runInBand",
    "test:e2e": "jest --testPathPatterns=e2e --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --ci --coverage --runInBand",
    "docker:build": "docker build -f Dockerfile -t cotizate-app:latest .",
    "docker:build:dev": "docker build -f docker/Dockerfile.dev -t cotizate-app:dev .",
    "docker:dev": "docker-compose -f docker/docker-compose.yml up",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "docker:clean": "docker-compose -f docker/docker-compose.yml down -v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmjalil96/cotizate-algo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/jmjalil96/cotizate-algo/issues"
  },
  "homepage": "https://github.com/jmjalil96/cotizate-algo#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nodemailer": "^7.0.5",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "slugify": "^1.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
